[Unit]
Description=HP Scan To (150 dpi duplex side A)

[Service]
User=hpscanto
Group=hpscanto
WorkingDirectory=/home/hpscanto/node-hp-scan-to
ExecStart=/home/hpscanto/.nvm/versions/node/v18.12.1/bin/node dist/index.js -ip HP40472D -d /home/hpscanto/duplex-feed/ -p '"side_a"' --suffix _150_duplex_side_A -r 150
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
